!$AMRVAC_DIR/setup.pl -d=33 -phi=0 -z=0 -g=14,14,14 -p=mhd 
 &testlist
 /

 &filelist
        primnames=  'rho v1 v2 v3 pth b1 b2 b3 psi'
        typefilelog='special'
        convert_type='vtuBCCmpi'
	slice_type  = 'vtu'
        saveprim=.true.
    	autoconvert=T
        nwauxio=1
	filenameout   = 'output/data'
	filenameini   = 'output/data'
	filenamelog   = 'output/amrvac'
 /

 &savelist
        itsave(1,1)=0
        itsave(1,2)=0
        itsave(1,3)=0
        ditsave(1)=1
        dtsave(2)=0.1
        dtsave(2)=0.1
        dtsave(3)=0.01
        nslices        = 3
	slicedir(1)    = 1
	slicedir(2)    = 2
	slicedir(3)    = 3
	slicecoord(1)  = 0.5d0
	slicecoord(2)  = 0.5d0
	slicecoord(3)  = 0.5d0
/

 &stoplist
        tmax=2.d0
        dtmin=1.d-6
 /
        tmaxexact=.true.

 &methodlist
        wnames=       'rho m1 m2 m3 e b1 b2 b3 psi'
        fileheadout=  'island_mhd22'
        typeadvance=  'threestep'
        typefull1=    13*'hllc'
        typelimiter1= 13*'mp5'
        useprimitive=.true.
        smallp=1.0d-14
        dimsplit=.false.
 /

 &boundlist
        dixB=    3
        typeB=   9*'periodic',9*'periodic',
                 9*'periodic',9*'periodic'
                 9*'periodic',9*'periodic'
 /

 &amrlist
        errorestimate=3
        mxnest=2
        tol=13*0.5d0
        nxlone1=128
        nxlone2=128
        nxlone3=128
        iprob=7
        xprobmin1=0.0d0
        xprobmax1=1.0d0
        xprobmin2=0.0d0
        xprobmax2=1.0d0
        xprobmin3=0.0d0
        xprobmax3=1.0d0
 /

 &paramlist
        courantpar=0.5d0
        dtdiffpar=0.5d0
        slowsteps=10
 /
